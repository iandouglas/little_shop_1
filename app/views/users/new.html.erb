<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>

<%= f.label :street %>
<%= f.text_field :street %>

<%= f.label :city %>
<%= f.text_field :city %>

<%= f.label :state %>
<%= f.text_field :state %>

<%= f.label :zip_code %>
<%= f.text_field :zip_code %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>

<%= f.submit 'Register Now' %>
<% end %>
